{
  "name": "kryber-backaend",
  "version": "0.0.2",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --inspect dist/src/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f ormconfig.cli.js",
    "migration:generate": "yarn run typeorm migration:generate -n",
    "migration:create": "yarn run typeorm migration:create -n",
    "migration:run": "yarn run typeorm migration:run",
    "migration:revert": "yarn run typeorm migration:revert"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/common": "~7.5.5",
    "@nestjs/core": "7.5.5",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "7.5.5",
    "@nestjs/schedule": "^0.4.2",
    "@nestjs/typeorm": "7.1.5",
    "@types/bcrypt": "^3.0.0",
    "@types/passport": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "4.9.0",
    "babel-eslint": "^10.1.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "exceljs": "^4.2.0",
    "nestjs-pino": "^1.2.0",
    "nodemailer": "^6.4.16",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "6.6.3",
    "states-us": "^1.0.1",
    "typeorm": "^0.2.25"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.3",
    "@nestjs/schematics": "7.2.2",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "4.17.8",
    "@types/jest": "^24.0.18",
    "@types/node": "14.14.10",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "8.0.11",
    "ts-node": "9.1.0",
    "tslint": "6.1.3",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@shared(.*)$": "<rootDir>/shared$1",
      "^@user(.*)$": "<rootDir>/users$1"
    }
  }
}
